{
	"version": "0.2.0",
	"configurations": [
        {
            "type": "electron",
            "request": "launch",
            "name": "Launch Electron against the workspace",
            "appDir": "${workspaceFolder}"
        },
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 9223,
			"webRoot": "${workspaceFolder}",
			"timeout": 30000,
			"preLaunchTask": "webpack-dev-server"
		}
	]
}